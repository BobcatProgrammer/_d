{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "node": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "prettier/prettier": [
      "error", {
        "trailingComma": "es5",
        "singleQuote": true
      }
    ],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0
  },
  "plugins": [
    "prettier"
  ]
}
